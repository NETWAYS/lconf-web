<?xml version="1.0" encoding="UTF-8"?>
<ae:configurations
	xmlns="http://agavi.org/agavi/config/parts/validators/1.0"
	xmlns:ae="http://agavi.org/agavi/config/global/envelope/1.0"
	parent="%core.module_dir%/AppKit/config/validators.xml"
>
	<ae:configuration>
		
		<validators>
			<validator class="number" name="user_id" required="true">
				<arguments base="user_id[]">
					<argument />
				</arguments>
			</validator>
			<validator class="isset" name="disable_only" required="false">
				<argument>disable_only</argument>			
			</validator>
		</validators>
		
	</ae:configuration>
</ae:configurations>
