<?xml version="1.0" encoding="UTF-8"?>
<ae:configurations
	xmlns="http://agavi.org/agavi/config/parts/validators/1.0"
	xmlns:ae="http://agavi.org/agavi/config/global/envelope/1.0"
	parent="%core.module_dir%/AppKit/config/validators.xml"
>
	<ae:configuration>
		
		<validators>
			<validator class="number" name="userId" required="false">
				<argument>userId</argument>
				<errors>
					<error>Validation of 'userId' failed</error>
				</errors>
			</validator>
			<validator class="number" name="groupId" required="false">
				<argument>groupId</argument>
				<errors>
					<error>Validation of 'userId' failed</error>
				</errors>
			</validator>
		</validators>
		
	</ae:configuration>
</ae:configurations>
