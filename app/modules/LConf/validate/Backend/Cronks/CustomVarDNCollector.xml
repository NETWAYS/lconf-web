<?xml version="1.0" encoding="UTF-8"?>
<ae:configurations
	xmlns="http://agavi.org/agavi/config/parts/validators/1.0"
	xmlns:ae="http://agavi.org/agavi/config/global/envelope/1.0"
	parent="%core.module_dir%/LConf/config/validators.xml"
>
	<ae:configuration>
		
		<validators>
			<validator class="string" name="target" required="true">
				<argument>target</argument>
				<errors>
					<error>Validation of 'field' failed</error>
				</errors>
			</validator>
			<validator class="string" name="target_field" required="true">
				<argument>target_field</argument>
				<errors>
					<error>Validation of 'field' failed</error>
				</errors>
			</validator>
			<validator class="string" name="ids" required="true">
				<argument>ids</argument>
				<errors>
					<error>Validation of 'field' failed</error>
				</errors>
			</validator>
		
		</validators>
		
	</ae:configuration>
</ae:configurations>
